<h1>Listing articles</h1>
<%= link_to 'New article', new_article_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
  
  <% @articles.each do |article| %>
    <tr>
      <th><%= article.title %></th>
      <th><%= article.text %></th>
      <th><%= link_to 'Show', article_path(article) %></th>
    </tr>
  <% end %>
</table>